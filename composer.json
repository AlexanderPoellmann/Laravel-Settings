{
    "name": "vendocrat/laravel-settings",
    "description": "Persistent, application-wide settings for Laravel 5.",
    "license": "MIT",
    "keywords": [
        "settings",
        "laravel"
    ],
    "authors": [
        {
            "name": "Alexander Manfred Poellmann"
        }
    ],
    "require": {
        "php"                : ">=5.5.9",
        "illuminate/support" : ">=5.1"
    },
    "require-dev": {
        "mockery/mockery"       : "0.9.*",
        "illuminate/database"   : ">=5.1",
        "illuminate/filesystem" : ">=5.1"
    },
    "autoload": {
        "classmap": [
            "database/migrations"
        ],
        "psr-4": {
            "vendocrat\\Settings\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
